{
  "name": "webtabs",
  "version": "2.0.0",
  "description": "Programa para exibir paginas dinamicamente.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pkg-win": "electron-packager . Webtabs --overwrite --platform=win32 --arch=all --icon=img/icon/win/icon.ico --prune=true --out=release_builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Webtabs\""
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "DrXama_PluginsManagerAPP"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Dr.Xam√£ - PluginsManager"
      }
    }
  },
  "repository": "https://github.com/GuilhermeSantos001/Webtabs.git",
  "author": "GuilhermeSantos001",
  "license": "CC0 1.0 Universal",
  "devDependencies": {
    "electron": "^5.0.3",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "electron-notify": "^0.1.0",
    "jquery": "^3.4.1",
    "materialize-css": "^1.0.0",
    "normalize-url": "^4.3.0",
    "nouislider": "^13.1.5",
    "ping": "^0.2.2"
  }
}